{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.tsconfig",
    "compilerOptions": {
        /* Environment */
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "jsx": "react-jsx",

        /* Bundler */
        "noEmit": true,

        /* Emit */
        "tsBuildInfoFile": "${configDir}/node_modules/.tmp/tsconfig.tsbuildinfo"
    }
}
